#!/bin/sh

export MY_HOST_NAME=
export MY_SITE_NAME=
export MY_SHELL_NAME=$MSYSTEM
export MY_OS_NAME=windows

if [ -z "$MY_WIN_HOME" ]; then
  echo "set MY_WIN_HOME"
  export MY_WIN_HOME=`cygpath -u $USERPROFILE`
fi

export MY_DESKTOP=$MY_WIN_HOME/Desktop
export MY_DOWNLOADS=$MY_WIN_HOME/Downloads

export MY_REMOTE_CONFIG=$MY_WIN_HOME/tktwr.github
export MY_LOCAL_CONFIG=$MY_WIN_HOME/tktwr.local

export MY_ENV=$MY_REMOTE_CONFIG/env
export MY_MEMO=$MY_REMOTE_CONFIG/memo
export MY_SAMPLES=$MY_REMOTE_CONFIG/samples
export MY_LIBTT=$MY_REMOTE_CONFIG/libtt
export MY_VIM=$MY_REMOTE_CONFIG/env/etc/vim

export MY_DIARY=$MY_LOCAL_CONFIG/secret/diary/diary.md.html
export MY_TODO=$MY_LOCAL_CONFIG/secret/diary/todo.md.html


export MY_PRIVATE=/x
export MY_PROTECTED=/y
export MY_PUBLIC=/z

export MY_REPO=$MY_PRIVATE/repo
export MY_WORK=$MY_PRIVATE/work
export MY_DOC=$MY_PRIVATE/work/doc
export MY_OFFICE=$MY_PRIVATE/office

export MY_DOWNLOADS2=$MY_PUBLIC/Downloads
export MY_OPT=$MY_PUBLIC/opt
export MY_PAPERS=$MY_PUBLIC/Papers
export MY_DATA=$MY_PUBLIC/data

export MY_BACKUP=$HOME/backup

export CC=clang
export CXX=clang++
