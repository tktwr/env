#!/bin/sh

GITBASH_GIT="/mingw64/bin/git.exe"

if [ -x "$GITBASH_GIT" ]; then
  exec "$GITBASH_GIT" "$@"
fi


BIN_DIR="$PROG_FILES_64_DIR/Git/mingw64/bin"
BIN_FILE="git.exe"

if [ ! -x "$BIN_DIR/$BIN_FILE" ]; then
  echo "no $BIN_DIR/$BIN_FILE"
  exit
fi

PATH="$PROG_FILES_64_DIR/Git/bin:$PATH"
PATH="$PROG_FILES_64_DIR/Git/usr/bin:$PATH"
PATH="$BIN_DIR:$PATH"

exec "$BIN_FILE" "$@"

