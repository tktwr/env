#!/bin/sh

GITBASH_GIT="/mingw64/bin/git.exe"

if [ -x "$GITBASH_GIT" ]; then
  exec "$GITBASH_GIT" "$@"
fi


BIN_FILE="$PROG_FILES_64_DIR/Git/mingw64/bin/git.exe"

if [ ! -x "$BIN_FILE" ]; then
  echo "no $BIN_FILE"
  exit
fi

PATH="$PROG_FILES_64_DIR/Git/bin:$PATH"
PATH="$PROG_FILES_64_DIR/Git/usr/bin:$PATH"
PATH="$PROG_FILES_64_DIR/Git/mingw64/bin:$PATH"

exec "$BIN_FILE" "$@"

